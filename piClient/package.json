{
  "name": "pi-client",
  "version": "dev",
  "private": true,
  "packageManager": "yarn@1.22.19",
  "scripts": {
    "ng": "ng",
    "start": "yarn ts-node ./script/prebuild.ts -e dev && ng serve --configuration dev",
    "build:dev": "yarn ts-node ./script/prebuild.ts -e dev && ng build --configuration dev",
    "build:prod": "yarn ts-node ./script/prebuild.ts -e prod && ng build --configuration prod",
    "test": "ng test",
    "lint:fix": "pretty-quick --staged && ng lint --fix",
    "e2e": "ng e2e",
    "prepare": "cd ../ && husky install piClient/.husky"
  },
  "dependencies": {
    "@angular/animations": "~14.0.1",
    "@angular/common": "~14.0.1",
    "@angular/compiler": "~14.0.1",
    "@angular/core": "~14.0.1",
    "@angular/platform-browser": "~14.0.1",
    "@angular/platform-browser-dynamic": "~14.0.1",
    "@angular/router": "~14.0.1",
    "chart.js": "^3.9.1",
    "material-design-icons": "^3.0.1",
    "ng2-charts": "3.0.11",
    "ngx-cookie-service": "^14.0.1",
    "rxjs": "~7.6.0",
    "zone.js": "~0.11.8"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~14.2.9",
    "@angular-eslint/builder": "14.1.2",
    "@angular-eslint/eslint-plugin": "14.1.2",
    "@angular-eslint/eslint-plugin-template": "15.0.0",
    "@angular-eslint/schematics": "14.1.2",
    "@angular-eslint/template-parser": "14.1.2",
    "@angular/cli": "~14.2.10",
    "@angular/compiler-cli": "~14.0.1",
    "@ngx-env/builder": "^2.2.0",
    "@types/jasmine": "~4.3.1",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@types/node": "^18.11.4",
    "@typescript-eslint/parser": "^5.44.0",
    "eslint": "^8.29.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsdoc": "^39.6.4",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "husky": "^8.0.2",
    "jasmine-core": "~4.5.0",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.4.1",
    "karma-chrome-launcher": "~3.1.1",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "^2.0.0",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3",
    "protractor": "~7.0.0",
    "ts-node": "^10.8.2",
    "typescript": "~4.7.4",
    "yargs": "^17.5.1",
    "yarn": "^1.22.19"
  }
}
