{
  "name": "pi-client",
  "version": "dev",
  "private": true,
  "packageManager": "yarn@1.22.19",
  "scripts": {
    "ng": "ng",
    "start": "yarn ts-node ./script/prebuild.ts -e dev && ng serve --configuration dev",
    "build:dev": "yarn ts-node ./script/prebuild.ts -e dev && ng build --configuration dev",
    "build:prod": "yarn ts-node ./script/prebuild.ts -e prod && ng build --configuration prod",
    "test": "ng test",
    "lint:fix": "pretty-quick --staged && ng lint --fix",
    "e2e": "ng e2e",
    "prepare": "cd ../ && husky install piClient/.husky"
  },
  "dependencies": {
    "@angular/animations": "~14.0.1",
    "@angular/common": "~14.0.1",
    "@angular/compiler": "~14.0.1",
    "@angular/core": "~14.0.1",
    "@angular/platform-browser": "~14.0.1",
    "@angular/platform-browser-dynamic": "~14.0.1",
    "@angular/router": "~14.0.1",
    "chart.js": "^3.9.1",
    "material-design-icons": "^3.0.1",
    "ng2-charts": "3.0.11",
    "ngx-cookie-service": "^14.0.1",
    "npm": "^9.6.0",
    "rxjs": "~7.8.1",
    "zone.js": "~0.11.8"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~14.2.9",
    "@angular-eslint/builder": "16.0.3",
    "@angular-eslint/eslint-plugin": "16.1.1",
    "@angular-eslint/eslint-plugin-template": "15.2.1",
    "@angular-eslint/schematics": "15.2.1",
    "@angular-eslint/template-parser": "16.0.3",
    "@angular/cli": "~15.2.10",
    "@angular/compiler-cli": "~14.0.1",
    "@ngx-env/builder": "^16.1.3",
    "@types/jasmine": "~4.3.1",
    "@types/node": "^20.5.1",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.5",
    "eslint": "^8.43.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jsdoc": "^46.5.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "husky": "^8.0.3",
    "jasmine-core": "~4.6.0",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.4.3",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "^2.0.0",
    "prettier": "^3.1.0",
    "pretty-quick": "^3.1.3",
    "protractor": "~7.0.0",
    "ts-node": "^10.8.2",
    "typescript": "~4.7.4",
    "yargs": "^17.7.2",
    "yarn": "^1.22.19"
  }
}
